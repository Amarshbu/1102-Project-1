    This program prompts the user to enter 
three numbers, then will find the biggest number
***************************************************/
#include <stdio.h>

int main()
{
  
    int n1, n2, n3;
  
    printf("Enter three numbers:\n");
    scanf("%d %d %d", &n1, &n2, &n3);
  
    if (n1 > n2)
    {
        if (n1 > n3)
            printf("\n%d is the bigger number", n1);
  
        else
            printf("\n%d is the bigger number", n3);
    }

    else
    {
        if (n2 > n3)
            printf("\n%d is the bigger number", n2);

        else
            printf("\n%d is the bigger number", n3);
    }

return 0;
}
